{
    "name": "bunqdesktop",
    "description": "A desktop implementation for the bunq API",
    "productName": "BunqDesktop",
    "homepage": "https://bunqdesktop.com/",
    "version": "0.8.1",
    "author": {
        "name": "BunqCommunity",
        "email": "bunqcommunity@gmail.com"
    },
    "license": "MIT",
    "main": "app/background.js",
    "supported_languages": [
        "en",
        "nl",
        "de",
        "es",
        "it"
    ],
    "build": {
        "appId": "com.bunq.desktop",
        "files": [
            "app/**/*",
            "node_modules/**/*",
            "package.json"
        ],
        "linux": {
            "category": "Office",
            "target": [
                {
                    "target": "AppImage",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                },
                {
                    "target": "deb",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                },
                {
                    "target": "snap",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        },
        "mac": {
            "category": "public.app-category.finance"
        },
        "publish": null
    },
    "scripts": {
        "webpack": "cross-env NODE_ENV=production webpack --progress --colors",
        "webpack:dev": "cross-env NODE_ENV=development webpack --debug --watch --progress --colors",
        "postinstall": "install-app-deps",
        "build": "gulp build",
        "dist": "build",
        "prerelease": "npm run update-release-files && gulp build --env=production && yarn run webpack",
        "release": "build",
        "start": "gulp start --env=development",
        "start:dev": "cross-env NODE_ENV=DEVELOPMENT gulp start --env=development",
        "update-icons": "node tasks/generate_material_icons.js",
        "update-translations": "node tasks/update_translations.js",
        "update-release-files": "node tasks/update_release_files.js",
        "add-contributer": "all-contributors add"
    },
    "publish": null,
    "dependencies": {
        "electron-localshortcut": "^2.0.2",
        "electron-log": "^2.2.9",
        "electron-settings": "^3.1.3",
        "fs-jetpack": "^1.0.0"
    },
    "devDependencies": {
        "@bunq-community/bunq-js-client": "^0.15.2",
        "@google-cloud/translate": "^1.1.0",
        "@types/axios": "^0.14.0",
        "@types/jest": "^22.1.3",
        "@types/moxios": "^0.4.8",
        "@types/node": "^9.4.6",
        "all-contributors-cli": "^4.11.1",
        "awaiting": "^2.0.1",
        "axios": "^0.18.0",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-react-html-attrs": "^2.0.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-system-import-transformer": "^3.1.0",
        "babel-plugin-transform-react-constant-elements": "^6.23.0",
        "babel-plugin-transform-react-inline-elements": "^6.22.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "chart.js": "^2.7.1",
        "clean-webpack-plugin": "^0.1.17",
        "cross-env": "^5.1.4",
        "css-loader": "^0.28.11",
        "date-fns": "^2.0.0-alpha.7",
        "electron": "^1.8.4",
        "electron-builder": "^20.2.0",
        "electron-reload": "^1.2.2",
        "email-validator": "^1.1.1",
        "gulp": "^3.9.1",
        "gulp-batch": "^1.0.5",
        "gulp-less": "^3.3.2",
        "gulp-plumber": "^1.1.0",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^4.3.11",
        "html-webpack-plugin": "^2.26.0",
        "i18next": "^10.6.0",
        "i18next-browser-languagedetector": "^2.1.1",
        "i18next-xhr-backend": "^1.5.1",
        "iban": "^0.0.8",
        "localforage": "^1.5.2",
        "loglevel": "^1.5.1",
        "material-ui": "^1.0.0-beta.39",
        "material-ui-icons": "^1.0.0-beta.17",
        "material-ui-pickers": "^1.0.0-rc.2",
        "mini-css-extract-plugin": "^0.4.0",
        "minimist": "^1.2.0",
        "mocha": "^3.4.2",
        "node-forge": "^0.7.1",
        "node-sass": "^4.6.0",
        "prop-types": "^15.5.8",
        "qrcode.react": "^0.7.2",
        "react": "^16.3.0",
        "react-chartjs-2": "^2.7.0",
        "react-color": "^2.14.0",
        "react-copy-to-clipboard": "^5.0.1",
        "react-countdown-now": "^1.3.0",
        "react-dom": "^16.3.0",
        "react-helmet": "^5.2.0",
        "react-i18next": "^7.5.1",
        "react-json-view": "^1.16.1",
        "react-loadable": "^5.3.1",
        "react-number-format": "^v3.0.0-beta3",
        "react-redux": "^5.0.7",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.0",
        "react-sticky-box": "^0.6.1",
        "react-visibility-sensor": "^3.11.0",
        "react-windowed-list": "^2.0.0",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "redux-mock-store": "^1.5.1",
        "redux-thunk": "^2.2.0",
        "rollup": "^0.45.2",
        "rollup-plugin-istanbul": "^1.1.0",
        "sass-loader": "^6.0.7",
        "source-map-support": "^0.4.15",
        "spectron": "^3.7.2",
        "store": "^2.0.4",
        "ts-jest": "^22.0.3",
        "ts-loader": "^4.1.0",
        "typescript": "^2.8.1",
        "uglifyjs-webpack-plugin": "^1.0.1",
        "universal-analytics": "^0.4.15",
        "webpack": "^4.4.1",
        "webpack-cli": "^2.0.13",
        "worker-loader": "^1.1.1"
    }
}
